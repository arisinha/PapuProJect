{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "DEEPSEEK_API_KEY": "@deepseek_api_key",
    "DEEPSEEK_MODEL": "deepseek-chat",
    "DEEPSEEK_API_BASE": "https://api.deepseek.com",
    "SERPAPI_API_KEY": "@serpapi_api_key",
    "AGENT_VERBOSE": "false",
    "AGENT_MAX_ITERATIONS": "10",
    "LLM_TEMPERATURE": "0.0",
    "LOG_LEVEL": "INFO"
  }
}
